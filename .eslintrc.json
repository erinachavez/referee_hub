{
    "parser" : "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true,
            "modules": true,
            "experimentalObjectRestSpread": true
        }
    },
    "env": {
        "es6": true
    },
    "plugins": [
        "react"
    ],
    "extends": [
        "airbnb",
        "plugin:react/recommended"
    ],
    "rules": {
        "semi": 0,
        "comma-dangle": 0
    }
}